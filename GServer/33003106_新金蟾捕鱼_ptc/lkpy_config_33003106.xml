	<Fish kind="12" name="蝴蝶鱼" speed="4" multiple="15" BoundingBox="180,70" hitRadius="10" captureProbability="0.06" />
	<Fish kind="13" name="孔雀鱼" speed="4" multiple="18" BoundingBox="255,88" hitRadius="10" captureProbability="0.05" />
	<Fish kind="14" name="剑鱼儿" speed="4" multiple="20" BoundingBox="250,80" hitRadius="10" captureProbability="0.04" />
