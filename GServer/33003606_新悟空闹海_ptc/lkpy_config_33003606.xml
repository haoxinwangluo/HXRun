		<!-- kind:对应BulletKind name:描述 speed:子弹速度 netRadius:渔网的半径 -->
	<Bullet kind="0" name="1炮筒" speed="25" netRadius="70" />
	<Bullet kind="1" name="2炮筒" speed="25" netRadius="100" />
	<Bullet kind="2" name="3炮筒" speed="25" netRadius="90" />
