	<Fish kind="13" name="绿剑鱼" speed="1" multiple="25" BoundingBox="255,88" hitRadius="10" captureProbability="0.8" />
	
	<Fish kind="14" name="蓝蝠鱼" speed="4" multiple="30" BoundingBox="250,80" hitRadius="10" captureProbability="0.8" />
	<Fish kind="15" name="银鲨鱼" speed="3" multiple="35" BoundingBox="180,150" hitRadius="10" captureProbability="0.8" />
